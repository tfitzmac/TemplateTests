{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters":{
        "baseTime":{
            "type":"string",
            "defaultValue": "[utcNow('u')]"
        },
        "add3Years": {
            "defaultValue": "[dateTimeAdd('2020-04-07', 'P3Y') ]",
            "type": "string"
        },
        "subtract9Days": {
            "defaultValue": "[dateTimeAdd('2020-04-07', '-P9D') ]",
            "type": "string"
        },
        "add1Hour": {
            "defaultValue": "[dateTimeAdd('2020-04-07', 'PT1H') ]",
            "type": "string"
        },
    },
    "resources": [],
    "outputs": {
        "add3Years": {
            "value": "[parameters('add3Years')]",
            "type": "string"
        },
        "subtract9Days": {
            "value": "[parameters('subtract9Days')]",
            "type": "string"
        },
        "add1Hour": {
            "value": "[parameters('add1Hour')]",
            "type": "string"
        },
    }
}
